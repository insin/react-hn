{
  "name": "react-hn",
  "version": "1.5.0",
  "description": "React-powered frontend for Hacker News using its Firebase API",
  "author": "Jonny Buchanan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/insin/react-hn.git"
  },
  "scripts": {
    "build": "npm run lint && cp node_modules/sw-toolbox/sw-toolbox.js public/sw-toolbox.js && react build src/index.js public/build && npm run precache",
    "deploy": "gcloud preview app deploy",
    "lint": "eslint src server.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "node server.js",
    "postinstall": "npm run build",
    "serve": "nwb serve",
    "precache": "sw-precache --root=public --config=sw-precache-config.json"
  },
  "engines": {
    "node": "6.1.0"
  },
  "main": "server.js",
  "dependencies": {
    "babel": "5.8.38",
    "babel-core": "5.8.38",
    "ejs": "2.4.2",
    "eslint-config-jonnybuchanan": "2.0.3",
    "events": "1.1.0",
    "express": "4.14.0",
    "firebase": "2.4.2",
    "history": "2.1.1",
    "isomorphic-fetch": "2.2.1",
    "nwb": "0.11.0",
    "object-assign": "4.1.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-router": "2.4.0",
    "react-timeago": "3.0.0",
    "reactfire": "0.7.0",
    "scroll-behavior": "0.5.0",
    "setimmediate": "1.0.4",
    "sw-precache": "3.2.0",
    "sw-toolbox": "3.2.1",
    "url-parse": "1.1.1"
  }
}
